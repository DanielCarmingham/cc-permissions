{
  "$schema": "template.schema.json",
  "name": "postgres-mcp",
  "description": "PostgreSQL MCP Server tools for database queries",
  "category": "MCP Servers",
  "detection": {
    "mcpServers": ["postgres", "postgresql"]
  },
  "levels": {
    "restrictive": [
      // Read-only SQL
      { "command": "mcp__postgres__query", "type": "mcp", "description": "Execute read-only SQL query" }
    ],
    "standard": [],
    "permissive": []
  }
}
